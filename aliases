# unzip gbk version
alias unzipgbk='unzip -O GBK'

# return up directory
alias ..2="cd ../.."
alias ..3="cd ../../.."
alias ..4="cd ../../../.."
alias ..5="cd ../../../../.."

# Show history
alias history='fc -l 1'

# List directory contents
alias ls='ls --color=always'
alias ll='ls -lh'
alias la='ls -A'
alias lal='ls -Alh'
alias l='ls -CF'
alias grep='grep --color=always'

# Create web http server with python
alias server='python -m SimpleHTTPServer'

# tmux shortcut
alias t='tmux attach'
